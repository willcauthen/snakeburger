<div class="conatiner">
	<div class="row">
		<div class="col-sm-6 col-sm-offset-3">
			<div class="snake-head hidden"> 
				<hr>
				<h1>Meet the Team</h1>
				<h3>Meet the invaluable men and women upon whom we depend to serve you high-quality snake meat!</h3>
				<hr>
			</div>
		</div>
	</div>

	<% if (@chefs != nil)  %>
		<% @chefs.each do |chef| %>
			<div class="row">
				<div class="col-sm-8 col-sm-offset-2 col-xs-10 col-xs-offset-1 jumbotron">
					<strong>Name:</strong> <%= chef.name %> <br/>
					<strong>Position:</strong> <%= chef.position %> <br/>
					<strong>Description:</strong>
					<div class="jumbotron">
						<%= chef.description %>
					</div>
				</div>
			</div>
		<% end %>
	<% else %>
		<div class="jumbotron">
			<h2>Whoops! looks like no one works here!</h2>
		</div>
	<% end %>
</div>
