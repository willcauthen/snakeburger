<div class="conatiner">
	<div class="row">
		<div class="col-sm-8 col-sm-offset-2 col-xs-10 col-xs-offset-1 jumbotron">
			<h1>Meet the Team</h1>
			<p>this page is under construction! as is our team! contact the owner if you want in on this lucrative and exciting project</p>
		</div>
	</div>
	<% @chefs.each do |chef| %>
	<div class="row">
		<div class="col-sm-8 col-sm-offset-2 col-xs-10 col-xs-offset-1 jumbotron">
			<strong>Name:</strong> <%= chef.name %> <br/>
			<strong>Position:</strong> <%= chef.position %> <br/>
			<strong>Description:</strong>
			<div class="jumbotron">
				<%= chef.description %>
			</div>
		</div>
	</div>
	<% end %>
</div>
